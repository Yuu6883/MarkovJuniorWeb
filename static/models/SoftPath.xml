<sequence values="BADYUGRW" origin="True">
  <all>
    <rule in="ABB" out="**D"/>
    <rule in="DBB" out="**D"/>
  </all>
  <one in="D" out="Y" steps="1" temperature="20.0">
    <field for="Y" from="A" on="BD"/>
  </one>
  <one in="D" out="G" steps="1" temperature="20.0">
    <field for="G" from="Y" on="BD"/>
  </one>
  <prl>
    <rule in="D" out="B"/>
    <rule in="A" out="B"/>
  </prl>
  <one temperature="2.0">
    <rule in="YBB" out="UWR"/>
    <rule in="RBB" out="WWR"/>
    <rule in="RBG" out="WWG"/>
    <field for="R" to="G" on="BR"/>
  </one>
</sequence>

