<sequence values="BkAENgo" symmetry="(xy)" origin="True">
    <one in="k * *" out="B k *" />
    <prl in="B" out="A" />
    <prl in="A * *" out="B * *" />
    <prl in="A A" out="E N" />
    <one in="* k" out="k k" steps="10" p="0.5" />
    <one in="B k" out="o k" />
    <one steps="50">
        <rule in="***/*B*/*** BBB/BoB/BBB" out="***/*o*/*** ***/*o*/***"></rule>
        <rule in="***/*B*/*** BBB/BoB/BBB" out="***/***/*** ***/*oo/***"></rule>
        <rule in="***/*B*/*** BBB/BoB/BBB" out="***/***/*o* ***/*o*/***"></rule>
        <rule in="***/*B*/*** BBB/BoB/BBB" out="***/***/**o ***/*o*/***"></rule>
    </one>
    <one in="Bo" out="go" />
    <one in="Bg" out="gg" symmetry="(xyz)" steps="50" temperature="1.0" >
        <field for="g" on="B" to="o" />
    </one>
    <prl in="g * * E" out="B B B E" />
    <prl in="Bo" out="Bg" symmetry="(xyz)" />
    <prl in="o" out="g" />
</sequence>