          <path from="R" to="r" on="A" color="p" inertia="True"/>
        </markov>
        <prl>
          <rule in="A" out="B"/>
          <rule in="k" out="B"/>
          <rule in="G" out="B"/>
          <rule in="r" out="B"/>
          <rule in="O p" out="* O"/>
          <rule in="b a" out="* E"/>
          <rule in="aax aBB aBB" out="**a *aa *a*"/>
        </prl>
        <prl comment="final colors">
